    <default-activity>about</default-activity>
    <managed-bean id="__1">
      <managed-bean-name>NonAdminHome</managed-bean-name>
      <managed-bean-class>com.oracle.apps.fleetmanagement.mobile.model.service.NonAdminHome</managed-bean-class>
      <managed-bean-scope>view</managed-bean-scope>
