<?xml version="1.0" encoding="UTF-8" ?>
<adfc-mobile-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.0">
  <task-flow-definition id="About-task-flow">
    <default-activity>LoginPage</default-activity>
    <view id="LoginPage">
      <page>/About/LoginPage.amx</page>
    </view>
    <view id="Signup">
      <page>/About/Signup.amx</page>
    </view>
    <view id="about">
      <page>/About/about.amx</page>
    </view>
    <control-flow-rule id="__1">
      <from-activity-id>LoginPage</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>signup</from-outcome>
        <to-activity-id>Signup</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__5">
        <from-outcome>signin</from-outcome>
        <to-activity-id>about</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__3">
      <from-activity-id>Signup</from-activity-id>
      <control-flow-case id="__4">
        <from-outcome>registrationDone</from-outcome>
        <to-activity-id>LoginPage</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <page-flow-scope-behavior>
      <push-new/>
    </page-flow-scope-behavior>
  </task-flow-definition>
</adfc-mobile-config>
