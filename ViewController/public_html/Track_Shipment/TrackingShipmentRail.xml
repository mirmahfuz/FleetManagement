<?xml version="1.0" encoding="UTF-8" ?>
<adfc-mobile-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.0">
  <task-flow-definition id="TrackingShipmentRail">
    <default-activity>Track_Shipment_Rail</default-activity>
    <initializer>#{pageFlowScope.TrackShipmentRailBean.init_trackingShipkment}</initializer>
    <managed-bean id="__1">
      <managed-bean-name>TrackShipmentRailBean</managed-bean-name>
      <managed-bean-class>com.oracle.apps.fleetmanagement.mobile.bean.TrackShipmentRailBean</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <view id="Track_Shipment_Rail">
      <page>/Track_Shipment/Track_Shipment_Rail.amx</page>
    </view>
    <method-call id="subscribe">
      <method>#{pageFlowScope.TrackShipmentRailBean.subscribeToMQTT}</method>
      <outcome>
        <fixed-outcome>success</fixed-outcome>
      </outcome>
    </method-call>
    <control-flow-rule id="__2">
      <from-activity-id>subscribe</from-activity-id>
      <control-flow-case id="__3">
        <from-outcome>success</from-outcome>
        <to-activity-id>Track_Shipment_Rail</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <page-flow-scope-behavior>
      <push-new/>
    </page-flow-scope-behavior>
  </task-flow-definition>
</adfc-mobile-config>
